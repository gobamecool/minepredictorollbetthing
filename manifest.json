{
  "manifest_version": 3,
  "name": "Rollbet Mines Predictor",
  "version": "1.0",
  "description": "Highlights safe/uncertain/mine tiles on Rollbet Mines.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.rollbet.gg/mines*"],
      "js": ["algorithms.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
